<div class="popup-custom-view">
  {{#if view.content.photo}}<img {{bind-attr src=view.content.photo}} class="popup-photo" />{{/if}}
  <span class="popup-title">{{view.content.displayTitle}}</span>
  {{#if view.content.popupDescription}}<div class="popup-desc">{{view.content.displayDescription}}</div>{{/if}}
  <div class="popup-country">{{view.content.displayCountry}}</div>
  {{#if view.content.url}}<a {{bind-attr href=view.content.url}} class="popup-url">{{t "base.website"}}</a>{{/if}}
  {{#if view.content.facebook}}<a {{bind-attr href=view.content.facebook}} class="popup-facebook">facebook</a>{{/if}}
  {{#if view.content.twitter}}<a {{bind-attr href=view.content.twitter}} class="popup-twitter">twitter</a>{{/if}}
  {{#if view.content.google}}<a {{bind-attr href=view.content.google}} class="popup-google">google</a>{{/if}}
</div>
